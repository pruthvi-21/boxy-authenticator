<?xml version="1.0" encoding="utf-8"?>
<com.libx.ui.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.libx.ui.preference.PreferenceCategory>
        <com.ps.tokky.preference.AppLockPreference
            app:defaultValue="false"
            app:key="@string/key_app_lock"
            app:summary="@string/summary_app_lock"
            app:title="@string/title_app_lock" />

        <com.libx.ui.preference.SwitchPreference
            app:defaultValue="false"
            app:key="@string/key_screenshots"
            app:summary="@string/summary_screenshots"
            app:title="@string/title_screenshots" />
    </com.libx.ui.preference.PreferenceCategory>

    <com.libx.ui.preference.PreferenceCategory app:title="@string/pref_cat_title_ui">
        <com.libx.ui.preference.SwitchPreference
            app:defaultValue="true"
            app:key="@string/pref_key_show_thumbnail"
            app:title="@string/pref_title_thumbnail" />
    </com.libx.ui.preference.PreferenceCategory>
</com.libx.ui.preference.PreferenceScreen>
